// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model secret{
  /// @Field(type => Int)
  id Int @id @default(autoincrement())
  /// @IsNotEmpty()
  ///@IsString()
  tenantId String
  /// @IsNotEmpty()
  ///@IsString()
  clientId String
  /// @IsNotEmpty()
  ///@IsString()
  clientSecret String
  /// @IsNotEmpty()
  ///@IsString()
  subscriptionId String
}